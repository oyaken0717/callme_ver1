<div class="user-index">
<div class="user-index__title">User一覧</div>
  <%= form_with(model: @users, method: :get, local: true, url: users_path) do |f| %>
    <div class="user-index__namesearch">
      <%= f.text_field :name %>
    </div>
    <div class="user-index__submit">
      <%= f.submit '検索' %>
    </div>
  <% end %><br>
  <div class="user-index__items">
    <% render @users %>
  </div>
</div>
<%#= render partial: "users/user", local: {users: @users} %>
<%#= render partial: "users/user", collection: @users as: "group" %>
